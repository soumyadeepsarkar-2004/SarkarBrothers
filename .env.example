# Google Gemini API Key
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# MongoDB Connection (optional - app works with mock data without it)
MONGODB_URI=mongodb://localhost:27017/toywonder

# Server Port
PORT=5000

# JWT Secret (change in production!)
JWT_SECRET=your-secret-key-change-in-production

# Frontend config (set to 'false' to use real backend API)
USE_MOCK_BACKEND=true

# API Base URL for frontend (leave empty for same-origin /api proxy)
API_BASE_URL=
